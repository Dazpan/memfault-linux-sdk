# Base URL as used in swupdate and memfaultd for reporting to Memfault
MEMFAULT_BASE_URL=https://device.memfault.com

# Memfault's Project key, taken from https://mflt.io/project-key
MEMFAULT_PROJECT_KEY=INSERT_PROJECT_KEY_HERE

# Device serial to report to Memfault
MEMFAULT_DEVICE_ID=qemu-tester

# Software version identifier to use for the binary we'll build and load on the QEMU device
MEMFAULT_SOFTWARE_VERSION=0.0.1

# Hardware version to report to Memfault
MEMFAULT_HARDWARE_VERSION=qemuarm64

# Software type to report to Memfault
MEMFAULT_SOFTWARE_TYPE=main